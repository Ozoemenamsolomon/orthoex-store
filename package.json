{
	"name": "orthoex-store",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:3001": "next dev --port 3001",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"lint": "next lint",
		"prepare": "husky install"
	},
	"dependencies": {
		"next": "11.1.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-markdown": "^8.0.3",
		"react-svg-star-rating": "^2.1.2",
		"react-tabs": "^4.2.1",
		"styled-components": "^5.3.1",
		"styled-icons": "^10.45.0",
		"swiper": "^8.4.4"
	},
	"devDependencies": {
		"@types/react": "17.0.27",
		"@types/react-tabs": "^2.3.4",
		"@types/styled-components": "^5.1.15",
		"babel-plugin-styled-components": "^1.13.2",
		"eslint": "^8.25.0",
		"eslint-config-next": "11.1.2",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"typescript": "4.4.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,tsx,ts}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write",
			"git add"
		]
	}
}
